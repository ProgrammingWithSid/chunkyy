{
  "name": "chunkyyy",
  "version": "0.1.0",
  "description": "AST-based semantic code chunking engine for TypeScript and JavaScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "chunkyyy": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "benchmark": "ts-node src/benchmark/index.ts",
    "api": "ts-node src/api/server.ts"
  },
  "keywords": [
    "ast",
    "chunking",
    "code-analysis",
    "rag",
    "typescript",
    "javascript",
    "semantic-chunking"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/parser": "^7.23.0",
    "@babel/traverse": "^7.23.0",
    "@babel/types": "^7.23.0",
    "@swc/core": "^1.7.0",
    "chokidar": "^3.5.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "esprima": "^4.0.1",
    "express": "^4.18.2",
    "fast-glob": "^3.3.2",
    "tree-sitter-wasms": "^0.1.13",
    "typescript": "^5.3.0",
    "web-tree-sitter": "^0.25.10"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/esprima": "^4.0.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
